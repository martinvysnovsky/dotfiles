{
  "type": {
    "prefix": "tp",
    "body": ["\"$1\"", "type $2: {", "  $3", "}"],
    "description": "Add type"
  },
  "extendType": {
    "prefix": "et",
    "body": ["extend type $1 {", "  $2", "}"],
    "description": "Extend any type"
  },
  "extendTypeQuery": {
    "prefix": "etq",
    "body": ["extend type Query {", "  $1", "}"],
    "description": "Extend type Query"
  },
  "extendTypeMutation": {
    "prefix": "etm",
    "body": ["extend type Mutation {", "  $1", "}"],
    "description": "Extend type Mutation"
  },
  "id": {
    "prefix": "id",
    "body": ["\"Id\"", "id: ID!"],
    "description": "Add id property"
  },
  "property": {
    "prefix": "prop",
    "body": ["\"$1\"", "$2: $3"],
    "description": "Add any property"
  }
}
